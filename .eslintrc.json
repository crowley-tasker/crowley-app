{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier/react"
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "env": {
        "jest": true
    },
    "rules": {
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/prop-types": 0,
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true
            }
        ]
    }
}
